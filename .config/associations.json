{
  "Term": "alacritty --class floatwin -e",
  "Handlers": [
    {
      "GlobPatterns": [
        "*"
      ],
      "Exec": "nvim",
      "Term": true
    },
    {
      "GlobPatterns": [
        "*.tar*",
        "*.gz",
        "*.zip"
      ],
      "Exec": "atool -x",
      "Term": true
    },
		{
			"GlobPatterns": [
				"*.png",
				"*.jp?g",
				"*.webm",
				"*.gif"
			],
			"Exec": "nomacs"
		},
    {
      "RegexPatterns": [
        "^https?://.*"
      ],
      "Exec": "firefox-devedition",
      "Term": false
    },
    {
			"GlobPatterns": [
				"*.mp4",
				"*.mkv"
			],
      "Exec": "mpv"
    }
  ]
}
